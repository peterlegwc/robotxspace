<form id="apply-form">
  <div class="row">
    <div class="col s12">
      <h5 class="blue-text">RobotX Space Tour Appointment</h5>
      <div>
        <small>We want to show you our amazing space! Please fill in the required fields and we will get in touch within 24 hours</small>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="input-field col s12">
      <input id="Field6" name="Field6" type="text" class="validate" required>
      <label for="Field6">Company*</label>
    </div>
  </div>
  <div class="row">
    <div class="input-field col s6">
      <input id="Field1" name="Field1" type="text" class="validate" required>
      <label for="Field1">First Name*</label>
    </div>
    <div class="input-field col s6">
      <input id="Field2" name="Field2" type="text" class="validate" required>
      <label for="Field2">Last Name*</label>
    </div>
  </div>
  <div class="row">
    <div class="input-field col s6">
      <input id="Field3" name="Field3" type="email" class="validate" required>
      <label for="email">Email*</label>
    </div>
    <div class="input-field col s2">
      <input id="Field4" name="Field4" type="tel" class="validate" placeholder="###" maxlength="3">
      <label for="Field1">Phone</label>
    </div>
    <div class="input-field col s2">
      <input id="Field4-1" name="Field4-1" type="tel" class="validate" placeholder="###" maxlength="3">
    </div>
    <div class="input-field col s2">
      <input id="Field4-2" name="Field4-2" type="tel" class="validate" placeholder="####" maxlength="4">
    </div>
  </div>
  <div class="row">
    <div class="input-field col s6">
      <input type="date" id="date" name="date" class="datepicker" placeholder="MM/DD/YYYY">
      <label for="date">Date</label>
    </div>
    <div class="input-field col s2">
      <select id="Field9" name="Field9">
        <option value="" disabled selected>HH</option>
        <option value="01">01</option>
        <option value="02">02</option>
        <option value="03">03</option>
        <option value="04">04</option>
        <option value="05">05</option>
        <option value="06">06</option>
        <option value="07">07</option>
        <option value="08">08</option>
        <option value="09">09</option>
        <option value="10">10</option>
        <option value="11">11</option>
        <option value="12">12</option>
      </select>
      <label>Hour</label>
    </div>
    <div class="input-field col s2">
      <select id="Field9-1" name="Field9-1">
        <option value="" disabled selected>MM</option>
        <option value="00">00</option>
        <option value="15">15</option>
        <option value="30">30</option>
        <option value="45">45</option>
      </select>
      <label>Minute</label>
    </div>
    <div class="input-field col s2">
      <select id="Field9-3" name="Field9-3">
        <option value="" disabled selected>AM/PM</option>
        <option value="AM">AM</option>
        <option value="PM">PM</option>
      </select>
      <label>AM/PM</label>
    </div>
  </div>
  <div class="row">
    <div class="input-field col s12">
      <textarea id="Field7" name="Field7" class="materialize-textarea"></textarea>
      <label for="message">Questions or Comments</label>
    </div>
  </div>
  <input type="hidden" name="_subject" value="RobotX Space Inquiry">
  <input type="text" name="_gotcha" style="display:none">
  <button class="btn waves-effect waves-light" type="submit" name="submit">Submit
  </button>
</form>
